<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://eventor.cz</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>eventor.cz</MSDeployServiceURL>
    <DeployIisAppPath>Eventor</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>eventor</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="EventDatabase" Order="1" Enabled="False">
          <Destination Path="Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\EventDatabase.mdf;Initial Catalog=EventDB;Integrated Security=True" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Eventor.Models.EventDbContext, Eventor" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="Eventor.Models.EventorUserDbContext" Order="2" Enabled="False">
          <Destination Path="Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Eventor.Models.EventorUserDbContext, Eventor" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="UserDatabase" Order="3" Enabled="False">
          <Destination Path="Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True" Name="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\UserDatabase_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)EventDatabase-Web.config Connection String">
      <ParameterValue>Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\EventDatabase.mdf;Initial Catalog=EventDB;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Eventor.Models.EventorUserDbContext-Web.config Connection String">
      <ParameterValue>Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)UserDatabase-Web.config Connection String">
      <ParameterValue>Data Source=.\SQLSERVER;AttachDbFilename=|DataDirectory|\UserDatabase.mdf;Initial Catalog=userDB;Integrated Security=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="Eventor.Models.EventorUserDbContext" />
  </ItemGroup>
</Project>